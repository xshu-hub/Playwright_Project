      "email": "user1@example.com",
      "role": "user",
      "status": "active",
      "description": "普通用户账户"
    },
    {
      "username": "user2",
      "password": "user123",
      "name": "测试用户2",
      "email": "user2@example.com",
      "role": "user",
      "status": "active",
      "description": "普通用户账户"
    },
    {
      "username": "manager",
      "password": "manager123",
      "name": "部门经理",
      "email": "manager@example.com",
      "role": "manager",
      "status": "active",
      "description": "部门经理账户，拥有审批权限"
    },
    {
      "username": "disabled_user",
      "password": "disabled123",
      "name": "禁用用户",
      "email": "disabled@example.com",
      "role": "user",
      "status": "inactive",
      "description": "已禁用的用户账户"
    }
  ],
  "test_scenarios": {
    "login": {
      "valid_credentials": [
        {"username": "admin", "password": "admin123", "expected_role": "admin"},
        {"username": "user1", "password": "user123", "expected_role": "user"},
        {"username": "manager", "password": "manager123", "expected_role": "manager"}
      ],
      "invalid_credentials": [
        {"username": "admin", "password": "wrong_password", "expected_error": "密码错误"},
        {"username": "nonexistent", "password": "any_password", "expected_error": "用户不存在"},
        {"username": "disabled_user", "password": "disabled123", "expected_error": "账户已禁用"}
      ]
    },
    "user_management": {
      "new_users": [
        {
          "username": "newuser1",
          "password": "newpass123",
          "name": "新用户1",
          "email": "newuser1@example.com",
          "role": "user",
          "expected_result": "success"
        },
        {
          "username": "admin",
          "password": "duplicate123",
          "name": "重复用户名",
          "email": "duplicate@example.com",
          "role": "user",
          "expected_result": "duplicate_username_error"
        }
      ],
      "user_updates": [
        {
          "target_username": "user1",
          "new_name": "更新的用户名",
          "new_email": "updated@example.com",
          "expected_result": "success"
        }
      ]
    }
  }
